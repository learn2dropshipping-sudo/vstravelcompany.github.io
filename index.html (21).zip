<body>
<import type React from "react"
import type { Metadata } from "next"
import { Noto_Sans as Noto } from "next/font/google"
import { Analytics } from "@vercel/analytics/next"
import "./globals.css"

const notoSansDevanagari = Noto({
  subsets: ["devanagari"],
  weight: ["400", "500", "600", "700"],
})

export const metadata: Metadata = {
  title: "V.S. Travel Company - पर्सनल ड्राइवर भर्ती दिल्ली",
  description:
    "सरकारी अधिकारियों के परिवार के लिए निजी ड्राइवर भर्ती - दिल्ली। 12 वर्ष का अनुभव। ₹30,000 प्रति माह वेतन। लॉन्ग टर्म कॉन्ट्रैक्ट।",
  keywords: "ड्राइवर भर्ती, निजी ड्राइवर, दिल्ली नौकरी, सरकारी ड्राइवर, driver job delhi",
}

export default function RootLayout({
  children,
}: Readonly<{
  children: React.ReactNode
}>) {
  return (
    <html lang="hi">
      <body className={`${notoSansDevanagari.className} antialiased`}>
        {children}
        <Analytics />
      </body>
    </html>
  )
}
import { Header } from "@/components/header"
import { HeroSection } from "@/components/hero-section"
